{
  "name": "sirona-api",
  "version": "1.0.0",
  "description": "The API that powers Sirona",
  "main": "app.js",
  "engines": {
    "node": "18.15.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/app.ts --unhandled-rejections=strict",
    "debug": "node --inspect -r ts-node/register -r source-map-support/register src/app.ts --unhandled-rejections=strict",
    "start-local-mongo": "docker run -d -p 27017:27017 mongo:6.0",
    "start-local-redis": "docker run -d -p 6379:6379 redis"
  },
  "author": "Benjamin Pike",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.0.0",
    "@sirona/utils": "file:../utils",
    "aws-sdk": "2.1472.0",
    "axios": "1.3.5",
    "bcrypt": "5.1.0",
    "body-parser": "1.20.2",
    "cookie-parser": "1.4.6",
    "express": "4.18.2",
    "file-type": "16.5.4",
    "google-auth-library": "8.7.0",
    "he": "1.2.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "9.0.0",
    "limiter": "2.1.0",
    "lru-cache": "9.0.1",
    "luxon": "3.3.0",
    "mongoose": "7.0.3",
    "multer": "1.4.5-lts.1",
    "nodemon": "2.0.22",
    "pako": "2.1.0",
    "redis": "4.6.5",
    "sharp": "0.32.6",
    "typescript": "5.0.3",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@sirona/types": "file:../types",
    "@types/bcrypt": "5.0.0",
    "@types/body-parser": "1.19.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/he": "1.2.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/luxon": "3.3.0",
    "@types/multer": "1.4.8",
    "@types/pako": "2.0.0",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "http-status-enum": "1.0.2",
    "prettier": "2.8.7",
    "prisma": "5.0.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1"
  }
}
